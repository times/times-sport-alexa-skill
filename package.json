{
  "name": "times-sport-alexa-skill",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint .",
    "deploy": "serverless deploy"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "serverless": "^1.27.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {}
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "rss-parser": "^3.1.2",
    "universal-analytics": "^0.4.17"
  }
}
